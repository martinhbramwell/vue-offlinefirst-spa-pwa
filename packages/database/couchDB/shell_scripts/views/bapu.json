{
  "_id": "_design/bapu",
  "views": {
    "latestInvoice": {
      "map": "function (doc) {\n  if ((doc.meta && doc.meta.handler && doc.meta.handler === 'InvoiceCreate') || (doc.data && doc.data.type && doc.data.type === 'invoice') && doc.data.fecha) { emit(doc.data.fecha, doc.data.seqib) };\n}"
    },
    "refreshedPersons": {
      "map": "function (doc) {\n  if (doc.data.type && doc.updated && doc.data.type == 'person') { emit(doc.updated, 1) };\n}",
      "reduce": "_count"
    },
    "allPersons": {
      "map": "function (doc) {\n  if (doc.data.type && doc.data.type === 'person') { emit(doc.data.type, 1) };\n}",
      "reduce": "_count"
    },
    "specialFix": {
      "map": "function (doc) {\n  if (doc._id === '00_SpecialFix') { emit(doc._id, doc) }\n}"
    }
  },
  "language": "javascript"
}
