    "dev": "node build/dev-server.js",
    "start": "node build/dev-server.js",
    "build": "NODE_ENV=production node build/build.js",
    "stage": "NODE_ENV=testing node build/build.js",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "PORT=8088 node test/e2e/nw/runner.js",
    "e2ew": "./node_modules/nodemon/bin/nodemon.js --exec 'npm run e2e'",
    "test": "npm run unit && npm run e2e",
    "lintf": "eslint --fix --ext .js,.vue src test/unit/specs test/e2e/nw/specs test/e2e/cy/integration",
    "lint": "eslint        --ext .js,.vue src test/unit/specs test/e2e/nw/specs test/e2e/cy/integration",
    "docs": "./node_modules/http-server/bin/http-server ./docs -p 8888",
    "tdocs": "./node_modules/http-server/bin/http-server ./tdocs -p 4567",
    "cyp": "cypress open",
    "ssi": "selenium-standalone install",
    "sss": "selenium-standalone start"
  },
